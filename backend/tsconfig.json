{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "strict": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "types": ["node"]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "prisma/**/*.ts"]
}
